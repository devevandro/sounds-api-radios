{
  "name": "sounds-api-radio",
  "version": "1.0.0",
  "description": "REST API to consume FM Radios",
  "author": "devevandro",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky install",
    "test:verbose": "vitest --passWithNoTests",
    "test:e2e": "NODE_ENV=test vitest --watch -c vitest.config.e2e.js",
    "test:unit": "vitest --watch --coverage",
    "test:ci": "vitest run --coverage",
    "test:staged": "vitest run",
    "infisical:dev": "infisical run start:dev --env=stage"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cli": "^8.1.5",
    "@nestjs/common": "^9.0.4",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/platform-express": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/joi": "^17.2.3",
    "@walletpass/pass-js": "^6.9.1",
    "axios": "^0.25.0",
    "bull": "^4.8.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.10.7",
    "googleapis": "^100.0.0",
    "joi": "^17.4.2",
    "mongoose": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@faker-js/faker": "^7.4.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^9.4.0",
    "@types/bull": "^3.15.8",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "vitest": "^0.30.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
